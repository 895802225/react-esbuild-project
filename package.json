{
    "name": "react-esbuild-project",
    "version": "1.0.0",
    "description": "corp cross-platform react district component",
    "scripts": {
        "watch": "esbuild src/app.jsx --outfile=build/index.js --bundle --watch",
        "tsw": "tsc --w",
        "start": " node ./esbuild.js && npm run tsw ",
        "build": "rm -rf lib && node ./esbuild.js && npm run ts-types"
    },
    "main": "lib/checkbox.js",
    "module": "lib/checkbox.js",
    "types": "lib/checkbox.d.ts",
    "files": [
        "dist"
    ],
    "dependencies": {
        "antd": "^4.20.2",
        "antd-mobile": "^5.11.2",
        "react": "^18.1.0",
        "react-dom": "^18.1.0"
    },
    "devDependencies": {
        "@craco/craco": "^6.4.3",
        "@deanc/esbuild-plugin-postcss": "^1.0.2",
        "@types/react": "^18.0.8",
        "@types/react-dom": "^18.0.3",
        "autoprefixer": "^10.4.7",
        "craco-esbuild": "^0.5.0",
        "esbuild": "^0.14.38",
        "esbuild-css-modules-plugin": "^2.2.16",
        "esbuild-plugin-postcss2": "^0.1.2",
        "fs-extra": "^10.1.0",
        "koa": "^2.13.4",
        "koa-static": "^5.0.0",
        "livereload": "^0.9.3",
        "postcss": "^8.4.13",
        "postcss-import": "^14.1.0",
        "postcss-px2rem": "^0.3.0",
        "postcss-px2rem-exclude": "0.0.6",
        "typescript": "^4.6.4"
    }
}
