        "esbuild-sass-plugin": "^2.6.0",
        "immer": "^9.0.15",
        "react": "18.1.0",
        "react-dom": "18.1.0",
        "use-immer": "^0.7.0"
    },
    "devDependencies": {
        "@craco/craco": "^7.1.0",
        "@deanc/esbuild-plugin-postcss": "1.0.2",
        "@types/react": "18.0.8",
        "@types/react-dom": "18.0.3",
